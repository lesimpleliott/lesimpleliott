{
  "name": "github-readme-generator",
  "version": "1.0.0",
  "description": "Outil de génération automatique du README de profil GitHub",
  "type": "module",
  "scripts": {
    "diff": "git diff --cached | pbcopy && echo '✅ Diff copied to clipboard !'",
    "start": "node scripts/cli/scenarioAuto.js",
    "fetch": "node scripts/cli/scenarioEdit.js",
    "rename": "node scripts/cli/scenarioRename.js",
    "generate": "node scripts/core/generateReadme.js"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "minimist": "^1.2.8",
    "node-fetch": "^3.3.2",
    "ora": "^8.2.0",
    "simple-git": "^3.28.0"
  }
}
